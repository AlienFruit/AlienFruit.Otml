<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>AlienFruit.Otml is an object tree model language .This package contains a parser. You can convert OTML to object model and vice versa.</Description>
    <Copyright>Copyright Â© Alexander Prokopovich 2018</Copyright>
    <Version>1.0.1</Version>
    <RepositoryUrl>https://github.com/AlienFruit/AlienFruit.Otml</RepositoryUrl>
    <PackageTags>otml</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GitVersionTask" Version="4.0.1-beta1-47">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
